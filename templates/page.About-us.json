    "17648338795881480f9e": {
      "type": "custom-html",
      "settings": {
        "color_scheme": "scheme-1",
        "html": "\u003cstyle\u003e\r\n  /* --- 全局容器 --- */\r\n  .fom-infra-section {\r\n    background-color: #f9f9f9;\r\n    padding: 80px 20px;\r\n    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;\r\n    color: #1a1a1a;\r\n  }\r\n\r\n  .fom-infra-wrapper {\r\n    max-width: 1400px;\r\n    margin: 0 auto;\r\n  }\r\n\r\n  /* --- 标题区域 --- */\r\n  .fom-infra-header {\r\n    text-align: center;\r\n    margin-bottom: 50px;\r\n    max-width: 800px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n\r\n  .fom-infra-title {\r\n    font-size: 36px;\r\n    font-weight: 700;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    margin-bottom: 15px;\r\n    color: #000;\r\n  }\r\n\r\n  .fom-infra-subtitle {\r\n    font-size: 16px;\r\n    color: #555;\r\n    line-height: 1.6;\r\n  }\r\n\r\n  /* --- ✨ [修改] 静态证书展示区域 ✨ --- */\r\n  .fom-cert-container {\r\n    margin-bottom: 80px;\r\n    /* 增加底部间距，与数据区隔开 */\r\n    padding: 0 20px;\r\n  }\r\n\r\n  /* 静态 Flex 布局，居中对齐 */\r\n  .fom-cert-row {\r\n    display: flex;\r\n    justify-content: center;\r\n    /* 水平居中 */\r\n    align-items: center;\r\n    /* 垂直居中 */\r\n    gap: 60px;\r\n    /* 图片之间的间距，可根据需要调整 */\r\n    flex-wrap: wrap;\r\n    /* 允许换行，适配小屏幕 */\r\n  }\r\n\r\n  .fom-cert-item {\r\n    /* 移除固定宽度，让图片自然展示，但限制最大宽以防失控 */\r\n    max-width: 250px;\r\n    transition: transform 0.3s ease;\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n\r\n  /* 悬停时的微动效：轻微上浮 */\r\n  .fom-cert-item:hover {\r\n    transform: translateY(-5px);\r\n  }\r\n\r\n  .fom-cert-img {\r\n    display: block;\r\n    width: auto;\r\n    /* ✨ [放大] 这里的 max-height 控制图片大小，从 80px 改为 130px */\r\n    max-height: 130px;\r\n    object-fit: contain;\r\n\r\n    /* 保持黑白到彩色的高级交互 */\r\n    filter: grayscale(100%);\r\n    opacity: 0.7;\r\n    transition: all 0.4s ease;\r\n  }\r\n\r\n  /* 悬停变彩 */\r\n  .fom-cert-item:hover .fom-cert-img {\r\n    filter: grayscale(0%);\r\n    opacity: 1;\r\n  }\r\n\r\n  /* --- 数据网格 --- */\r\n  .fom-infra-grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(3, 1fr);\r\n    gap: 30px;\r\n  }\r\n\r\n  .fom-infra-card {\r\n    background: #fff;\r\n    padding: 40px 30px;\r\n    border: 1px solid #eee;\r\n    text-align: left;\r\n    transition: all 0.4s cubic-bezier(0.25, 0.8, 0.25, 1);\r\n    position: relative;\r\n    overflow: hidden;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n  }\r\n\r\n  .fom-infra-card:hover {\r\n    transform: translateY(-8px);\r\n    box-shadow: 0 15px 35px rgba(0, 0, 0, 0.1);\r\n    border-color: #000;\r\n  }\r\n\r\n  .fom-stat-row {\r\n    display: flex;\r\n    align-items: baseline;\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  .fom-stat-number {\r\n    font-size: 60px;\r\n    font-weight: 900;\r\n    line-height: 1;\r\n    color: #1a1a1a;\r\n    letter-spacing: -1px;\r\n    display: inline-block;\r\n    font-variant-numeric: tabular-nums;\r\n  }\r\n\r\n  .fom-stat-unit {\r\n    font-size: 24px;\r\n    font-weight: 700;\r\n    color: #5b87c1;\r\n    margin-left: 8px;\r\n    text-transform: uppercase;\r\n  }\r\n\r\n  .fom-stat-desc {\r\n    font-size: 14px;\r\n    font-weight: 600;\r\n    color: #888;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1.5px;\r\n    padding-top: 15px;\r\n    border-top: 1px solid #f0f0f0;\r\n    width: 100%;\r\n  }\r\n\r\n  /* --- 移动端适配 --- */\r\n  @media (max-width: 1024px) {\r\n    .fom-infra-grid {\r\n      grid-template-columns: repeat(2, 1fr);\r\n    }\r\n  }\r\n\r\n  @media (max-width: 600px) {\r\n\r\n    /* 数据卡片变单列 */\r\n    .fom-infra-grid {\r\n      grid-template-columns: 1fr;\r\n    }\r\n\r\n    .fom-stat-number {\r\n      font-size: 48px;\r\n    }\r\n\r\n    /* 证书区域适配：手机端变为 2列布局 */\r\n    .fom-cert-row {\r\n      gap: 30px;\r\n    }\r\n\r\n    .fom-cert-item {\r\n      width: 45%;\r\n      /* 强制一行两个 */\r\n    }\r\n\r\n    .fom-cert-img {\r\n      max-height: 80px;\r\n      /* 手机上稍微小一点，不然会撑破 */\r\n    }\r\n  }\r\n\u003c/style\u003e\r\n\r\n\u003cdiv class=\"fom-infra-section\" id=\"stats-section\"\u003e\r\n  \u003cdiv class=\"fom-infra-wrapper\"\u003e\r\n\r\n    \u003cdiv class=\"fom-infra-header\"\u003e\r\n      \u003cdiv class=\"fom-infra-title\"\u003eOur Manufacturing Infrastructure\u003c/div\u003e\r\n      \u003cdiv class=\"fom-infra-subtitle\"\u003e\r\n        Scalable production capabilities backing your global supply chain with speed and precision.\r\n      \u003c/div\u003e\r\n    \u003c/div\u003e\r\n\r\n    \u003cdiv class=\"fom-infra-grid\"\u003e\r\n      \u003cdiv class=\"fom-infra-card\"\u003e\r\n        \u003cdiv class=\"fom-stat-row\"\u003e\r\n          \u003cspan class=\"fom-stat-number\" data-target=\"30\"\u003e0\u003c/span\u003e\r\n          \u003cspan class=\"fom-stat-unit\"\u003e+ Years\u003c/span\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class=\"fom-stat-desc\"\u003eExperience Since 1990\u003c/div\u003e\r\n      \u003c/div\u003e\r\n      \u003cdiv class=\"fom-infra-card\"\u003e\r\n        \u003cdiv class=\"fom-stat-row\"\u003e\r\n          \u003cspan class=\"fom-stat-number\" data-target=\"120\"\u003e0\u003c/span\u003e\r\n          \u003cspan class=\"fom-stat-unit\"\u003e+ Countries\u003c/span\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class=\"fom-stat-desc\"\u003eGlobal Reach\u003c/div\u003e\r\n      \u003c/div\u003e\r\n      \u003cdiv class=\"fom-infra-card\"\u003e\r\n        \u003cdiv class=\"fom-stat-row\"\u003e\r\n          \u003cspan class=\"fom-stat-number\" data-target=\"7\"\u003e0\u003c/span\u003e\r\n          \u003cspan class=\"fom-stat-unit\"\u003eDays\u003c/span\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class=\"fom-stat-desc\"\u003eRapid Prototyping\u003c/div\u003e\r\n      \u003c/div\u003e\r\n      \u003cdiv class=\"fom-infra-card\"\u003e\r\n        \u003cdiv class=\"fom-stat-row\"\u003e\r\n          \u003cspan class=\"fom-stat-number\" data-target=\"112\"\u003e0\u003c/span\u003e\r\n          \u003cspan class=\"fom-stat-unit\"\u003e+ Lines\u003c/span\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class=\"fom-stat-desc\"\u003eProduction Equipment\u003c/div\u003e\r\n      \u003c/div\u003e\r\n      \u003cdiv class=\"fom-infra-card\"\u003e\r\n        \u003cdiv class=\"fom-stat-row\"\u003e\r\n          \u003cspan class=\"fom-stat-number\" data-target=\"66700\"\u003e0\u003c/span\u003e\r\n          \u003cspan class=\"fom-stat-unit\"\u003eM²\u003c/span\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class=\"fom-stat-desc\"\u003eSmart Manufacturing Facility\u003c/div\u003e\r\n      \u003c/div\u003e\r\n      \u003cdiv class=\"fom-infra-card\"\u003e\r\n        \u003cdiv class=\"fom-stat-row\"\u003e\r\n          \u003cspan class=\"fom-stat-number\" data-target=\"5000000\"\u003e0\u003c/span\u003e\r\n          \u003cspan class=\"fom-stat-unit\"\u003e+ Yards\u003c/span\u003e\r\n        \u003c/div\u003e\r\n        \u003cdiv class=\"fom-stat-desc\"\u003eAnnual Production Capacity\u003c/div\u003e\r\n      \u003c/div\u003e\r\n    \u003c/div\u003e\r\n\r\n  \u003c/div\u003e\r\n\u003c/div\u003e\r\n\r\n\u003cscript\u003e\r\n  (function() {\r\n  document.addEventListener('DOMContentLoaded', function() {\r\n    \r\n    // --- 1. 数字滚动动画逻辑 (保留) ---\r\n    const statsSection = document.getElementById('stats-section');\r\n    const numbers = document.querySelectorAll('.fom-stat-number');\r\n    let hasAnimated = false; \r\n    const observer = new IntersectionObserver((entries, obs) =\u003e {\r\n      entries.forEach(entry =\u003e {\r\n        if (entry.isIntersecting \u0026\u0026 !hasAnimated) {\r\n          hasAnimated = true; \r\n          runAnimations();\r\n          obs.unobserve(entry.target); \r\n        }\r\n      });\r\n    }, { threshold: 0.2 });\r\n\r\n    if (statsSection) observer.observe(statsSection);\r\n\r\n    function runAnimations() {\r\n      numbers.forEach(num =\u003e {\r\n        const target = +num.getAttribute('data-target'); \r\n        const duration = 2000; \r\n        const frameRate = 16; \r\n        const totalFrames = Math.round(duration / frameRate);\r\n        let frame = 0;\r\n        const updateCount = () =\u003e {\r\n          frame++;\r\n          const progress = 1 - Math.pow(1 - frame / totalFrames, 4);\r\n          const currentVal = Math.round(target * progress);\r\n          num.innerText = currentVal.toLocaleString();\r\n          if (frame \u003c totalFrames) requestAnimationFrame(updateCount);\r\n          else num.innerText = target.toLocaleString(); \r\n        };\r\n        updateCount();\r\n      });\r\n    }\r\n\r\n    // ✨ [删除] 原有的轮播图 JS 逻辑已移除，确保图片静态展示 ✨\r\n  });\r\n})();\r\n\u003c/script\u003e",
