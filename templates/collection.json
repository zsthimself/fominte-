        "product-item.listInquiry .b2b-list-inquiry{background:transparent;padding:0}",
        "product-item.listInquiry .b2b-list-inquiry-btn{background:rgba(0,0,0,.4);border:1px solid #fff;color:#fff;border-radius:5px;font-weight:700;width:80%;padding:10px!important;margin:8px auto 0!important;display:flex!important;align-items:center!important;justify-content:center!important;transition:all .3s ease}",
